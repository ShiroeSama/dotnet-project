<Window x:Class="ShirOSApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:view="clr-namespace:ShirOSApp.View"
        xmlns:local="clr-namespace:ShirOSApp"
        mc:Ignorable="d"
        Title="MasterDetail" Height="350" Width="525">
    <Grid>
        <DockPanel>
            <Label DockPanel.Dock="Top" VerticalAlignment="Center" Margin="10">List Product</Label>
            <view:ListProduct DockPanel.Dock="Left" DataContext="{Binding ListProductVM}"></view:ListProduct>
        </DockPanel>
    </Grid>
</Window>
